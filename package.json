{
  "name": "dexter-server",
  "version": "0.1.0",
  "main": "server/server.ts",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "node --inspect=9229 --require ts-node/register --require source-map-support/register server/server.ts",
    "dev-client": "cd client && npm start",
    "build": "tsc",
    "start": "node dist/server/server.js",
    "clean": "rimraf dist",
    "debug": "node --inspect-brk=9229 --require ts-node/register --require source-map-support/register server/server.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "firebase-admin": "^12.6.0",
    "http-proxy-middleware": "^3.0.2",
    "link-preview-js": "^3.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "telegraf": "^4.16.3",
    "typescript": "^5.8.3",
    "urql": "^4.2.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.5",
    "@types/node-fetch": "^2.6.11",
    "@types/source-map-support": "^0.5.10",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.0.1",
    "ts-node": "^10.9.2"
  }
}
